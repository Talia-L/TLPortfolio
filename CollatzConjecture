import java.util.Scanner;

public class CollatzConject 
{
	public static void main(String[]args )
	{
	
	Scanner keyboard = new Scanner (System.in);
	
			int i;
			i = 100;
			
			while (i != 0)
			{
				if (i % 2 == 0) {
						i = i/2;
						System.out.println(i);
				}else{
						i = i+3 + 1;
						System.out.println(i);
				i --;
			}
			}	
	}
}
